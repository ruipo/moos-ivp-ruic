ServerHost = localhost
ServerPort = 9002

// Community name IS the vehicle name
Community  = Michael_2

MOOSTimeWarp = 20
//LatOrigin  = 43.825300 
//LongOrigin = -70.330400 
LatOrigin  = 42.3584
LongOrigin = -71.08745

  
//------------------------------------------------------------
Processconfig = ANTLER
{
  MSBetweenLaunches = 100
   
  Run = MOOSDB             @ NewConsole = false
  Run = uSimMarine         @ NewConsole = false
  Run = pLogger            @ NewConsole = false
  Run = pNodeReporter      @ NewConsole = false
  Run = pMarinePID         @ NewConsole = false
  Run = pHelmIvP           @ NewConsole = false
  Run = uProcessWatch      @ NewConsole = false

  Run = pShare             @ NewConsole = false
  Run = pHostInfo          @ NewConsole = false
  Run = uFldNodeBroker     @ NewConsole = false
  Run = uFldMessageHandler @ NewConsole = false
  
  Run = uTimerScript       @ NewConsole = false
  Run = pBasicContactMgr   @ NewConsole = false

  Run = pFrontEstimate     @ NewConsole = false
  //Run = uXMS      @ NewConsole = true
}

ProcessConfig = pFrontEstimate
 {
   AppTick    = 4
   CommsTick  = 4

   reporting_vname = "Rui_1"
   vname = Michael_2
   temperature_factor = 50
   cooling_steps = 1000
   min_offset = -120;
   max_offset = -60;
   min_angle = -5;
   max_angle = 15;
   min_amplitude = 0;
   max_amplitude = 50;
   min_period = 200;
   max_period = 600;
   min_wavelength = 100;
   max_wavelength = 500;
   min_alpha = 500;
   max_alpha = 500;
   min_beta = 10;
   max_beta = 30;
   min_T_N = 15;
   max_T_N = 25; 
   min_T_S = 20;
   max_T_S = 30; 
   concurrent = true  // Flag controlling whether the
                               // annealing is performed concurrently
                               // with survey.
   adaptive = false      // Uses adaptive simulated annealing, 
                               // where the parameter search interval 
                               // reduced proportionally to temperature 
 }

//--------------------------------------------------------
// uTimerScript Configuration Block

ProcessConfig = uTimerScript
{
  AppTick   = 4
  CommsTick = 4
  
  PAUSED     = false
  RESET_MAX  = unlimited
  RESET_TIME = end

  DELAY_RESET = 0.5

  //RANDVAR = varname=X, min=0,  max=100, key=at_reset
  //RANDVAR = varname=Y, min=0,  max=100, key=at_reset
  //EVENT = var=NODE_MESSAGE_LOCAL, val="src_node=Michael_2,dest_node=all,var_name=FOOBAR,string_val=$[X],$[Y]", time=0:30 

  EVENT = var=UCTD_SENSOR_REQUEST, val=vname=Michael_2, time=5 
}


 


